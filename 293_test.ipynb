    "# code multiplies each number in list x by each number in y \n",
    "# appends each product to list \"result\"\n",
    "# prints result list \n",
    "x = [10, 20, 30, 40, 50]\n",
    "y = [1, 2, 3, 4, 5]\n",
    "result = [] \n",
    "for i in x:\n",
    "    for j in y:\n",
    "        result.append(i*j)\n",
    "\n",
    "print(result)"
